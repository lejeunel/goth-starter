package home

import (
	"gothstarter/views/layouts"
)

templ Index(items []string) {
	@layouts.Base() {
        <p class="font-semibold">List of entries:</p>
		<div class="flex flex-col items-center">
        <ul>
        for _, item := range items {
            <li class="italic">{ item }</li>
        }
        </ul>
        </div>
	}
}
